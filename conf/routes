# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


# Test Routes
GET     /test/                      controllers.TestController.test()
GET     /table/                     controllers.TestController.testTableMaker()
GET     /parserCre                  controllers.TestController.parserCreate()
GET     /parserGet/:id              controllers.TestController.parserGet(id :Long)
GET     /parserDel/:id              controllers.TestController.parserDelete(id :Long)

# Admin
->      /admin                      admin.Routes
->      /admin/                     admin.Routes

# API
->      /api                        api.Routes


