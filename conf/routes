# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


# Test Routes
GET     /test/                      controllers.HomeController.testExtensionMaker()

# Admin
->      /admin                      admin.Routes

# API
->      /api                        api.Routes

# Web
# GET     /assets/lib/*file           controllers.Assets.versioned(path="/public/lib", file: Asset)
# GET     /                           controllers.Assets.at(path= "/public/dist", file="/index.html")
# GET     /web/*file                  controllers.HomeController.webApp(file: String)
# GET     /                           controllers.HomeController.index()
# GET     /*file                      controllers.HomeController.webAppRedirect(file: String)